{
  "description": "Créer un point de restauration nommé",
  "steps": [
    {
      "type": "command",
      "command": "node",
      "args": ["/home/studiosdb/mcp-server/backup-manager.js", "backup", "{{backup_name}}"],
      "timeout": 120000
    },
    {
      "type": "command",
      "command": "ls",
      "args": ["-lah", "/home/studiosdb/backups"],
      "timeout": 5000
    }
  ]
}